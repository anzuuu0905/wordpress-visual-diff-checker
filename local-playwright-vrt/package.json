{
  "name": "local-wordpress-vrt-playwright",
  "version": "1.0.0",
  "description": "Local WordPress VRT with Playwright - High Precision Screenshot Comparison",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node test.js",
    "install-browsers": "npx playwright install",
    "start:enterprise": "node enterprise-server.js",
    "benchmark": "node benchmark.js",
    "ultra": "node ultra-vrt.js",
    "ultra:demo": "node ultra-vrt.js",
    "performance:analyze": "node resource-analysis.js",
    "setup:gpu": "npm install canvas @tensorflow/tfjs-node-gpu",
    "setup:enterprise": "npm install && npm run install-browsers && npm run setup:gpu"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "googleapis": "^159.0.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.2",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7",
    "pixelmatch": "^5.3.0",
    "playwright": "^1.40.0",
    "pngjs": "^7.0.0",
    "puppeteer-pdf": "^1.2.0",
    "sharp": "^0.32.0",
    "p-limit": "^3.1.0",
    "lru-cache": "^10.0.0",
    "cluster": "^0.7.7",
    "bull": "^4.11.5",
    "redis": "^4.6.10"
  },
  "devDependencies": {
    "nodemon": "^3.0.0"
  },
  "keywords": [
    "wordpress",
    "vrt",
    "visual-regression-testing",
    "playwright",
    "local",
    "screenshot-comparison"
  ],
  "author": "Claude Code",
  "license": "MIT"
}
